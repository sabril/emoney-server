<%= simple_form_for @sync, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :data, as: :text %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                admin_syncs_path, :class => 'btn btn-default' %>
<% end %>
