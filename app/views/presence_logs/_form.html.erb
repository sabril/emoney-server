<%= simple_form_for @presence_log, :html => { :class => 'form-vertical' } do |f| %>
  <%= f.input :accn %>
  <%= f.input :timestamp %>
  <%= f.input :imei %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                presence_logs_path, :class => 'btn btn-default' %>
<% end %>
