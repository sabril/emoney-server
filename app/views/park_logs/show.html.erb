<%- model_class = ParkLog -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:accn) %>:</strong></dt>
  <dd><%= @park_log.accn %></dd>
  <dt><strong><%= model_class.human_attribute_name(:timestamp) %>:</strong></dt>
  <dd><%= @park_log.timestamp %></dd>
  <dt><strong><%= model_class.human_attribute_name(:park) %>:</strong></dt>
  <dd><%= @park_log.park %></dd>
  <dt><strong><%= model_class.human_attribute_name(:imei) %>:</strong></dt>
  <dd><%= @park_log.imei %></dd>
  <dt><strong><%= model_class.human_attribute_name(:sesn) %>:</strong></dt>
  <dd><%= @park_log.sesn %></dd>
  <dt><strong><%= model_class.human_attribute_name(:account_id) %>:</strong></dt>
  <dd><%= @park_log.account_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              park_logs_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_park_log_path(@park_log), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              park_log_path(@park_log),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
