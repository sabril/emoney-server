<%= simple_form_for @park_log, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :accn %>
  <%= f.input :timestamp %>
  <%= f.input :park %>
  <%= f.input :imei %>
  <%= f.input :sesn %>
  <%= f.input :account_id %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                park_logs_path, :class => 'btn btn-default' %>
<% end %>
