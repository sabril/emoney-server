<%= simple_form_for @account, as: "account", :html => { :class => 'form-vertical' } do |f| %>
  <%= f.input :accn %>
  <%= f.input :imei %>
  <%= f.input :balance %>
  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                accounts_path, :class => 'btn btn-default' %>
<% end %>
